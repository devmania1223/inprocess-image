{
    "name": "timesheet-api",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "lint": "eslint --ignore-path .eslintignore .",
        "fix": "eslint --fix --ignore-path .eslintignore .",
        "watch": "nodemon ./bin/www",
        "format": "prettier --write .",
        "dbcreate": "node_modules/.bin/sequelize --config src/config/config.js db:create",
        "dbmigrate": "node_modules/.bin/sequelize --config src/config/config.js db:migrate"
    },
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "crypto-random-string": "^3.3.0",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "express-rate-limit": "^5.0.0",
        "gen-uid": "0.0.2",
        "helmet": "^3.18.0",
        "http-status-codes": "^2.1.4",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "morgan": "~1.9.1",
        "mysql2": "^1.6.5",
        "nodemailer": "^6.4.14",
        "sequelize": "^5.21.2",
        "swagger-generator-express": "^2.1.1",
        "swagger-ui-express": "^4.0.7"
    },
    "devDependencies": {
        "dotenv": "^8.2.0",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.13.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "nodemon": "^1.19.4",
        "prettier": "^2.1.2",
        "sequelize-cli": "^5.5.1"
    }
}
